Cell[TextData[{
 "Define a simplification function based on ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", "p", "\[LessEqual]", "1"}], TraditionalForm]],
  
  FormatType->"TraditionalForm"]
}], "Text",
 CellChangeTimes->{{3.726606768676527*^9, 3.726606800087954*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"FS24b", "[", "aa_", "]"}], ":=", 
  RowBox[{"FullSimplify", "[", 
   RowBox[{"aa", ",", 
    RowBox[{"Assumptions", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"p", "\[Element]", "Reals"}], ",", 
       RowBox[{"0", "\[LessEqual]", "p", "\[LessEqual]", "1"}]}], "}"}]}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.7266065468889503`*^9, 3.72660658936723*^9}}],

Cell["\<\
Define the binary entropy function in terms of base 2 logarithms\
\>", "Text",
 CellChangeTimes->{{3.7266068071992598`*^9, 3.72660684564253*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"H", "[", "pp_", "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"-", "pp"}], " ", 
    RowBox[{"Log", "[", 
     RowBox[{"2", ",", "pp"}], "]"}]}], "-", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"1", "-", "pp"}], ")"}], 
    RowBox[{"Log", "[", 
     RowBox[{"2", ",", 
      RowBox[{"(", 
       RowBox[{"1", "-", "pp"}], ")"}]}], 
     "]"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"H", "[", "p", "]"}], "//", "FS24b"}]}], "Input",
 CellChangeTimes->{{3.726606594076213*^9, 3.726606647233852*^9}}],

Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "1"}], "+", "p"}], ")"}], " ", 
    RowBox[{"Log", "[", 
     RowBox[{"1", "-", "p"}], "]"}]}], "-", 
   RowBox[{"p", " ", 
    RowBox[{"Log", "[", "p", "]"}]}]}], 
  RowBox[{"Log", "[", "2", "]"}]]], "Output",
